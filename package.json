  "bugs": {
    "url": "https://github.com/andrewinsoul/MyDiary/issues"
  },
  "homepage": "https://github.com/andrewinsoul/MyDiary#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.0",
    "pg": "^7.4.3",
    "swagger-ui-express": "^3.0.10",
